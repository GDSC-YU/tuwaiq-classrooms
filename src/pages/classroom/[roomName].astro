---
import Layout from "../../layouts/Layout.astro";
import { Room, rooms } from "../../data/rooms";
import SchedulePage from "../../components/SchedulePage.svelte";

const { roomName } = Astro.params;
const roomNameString = roomName!.toString();
const chosenRoom: Room = rooms[roomNameString];

export function getStaticPaths() {
  const roomKeys = Object.keys(rooms);

  return roomKeys.map((key) => ({
    params: { roomName: key },
  }));
}
---

<Layout title={roomNameString}>
  <SchedulePage room={chosenRoom} client:only="svelte" />
</Layout>
